<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="nsu.chebotareva.task_2_3_1.SnakeController" alignment="CENTER" spacing="10">
  <Canvas fx:id="gameCanvas" width="720" height="600"/>
  <ComboBox fx:id="robotCountComboBox" promptText="Выберите количество роботов">
    <items>
      <FXCollections fx:factory="observableArrayList">
        <String fx:value="0"/>
        <String fx:value="1"/>
        <String fx:value="2"/>
        <String fx:value="3"/>
      </FXCollections>
    </items>
  </ComboBox>
  <Button fx:id="restartButton" text="Рестарт"/>
</VBox>